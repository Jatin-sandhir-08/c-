#include <iostream>
using namespace std;

//define a function that will return factorial of any int n
int fac(int n){
    int i = 1;
    int pro = 1;
    while (i<=n){
        pro = pro*i;
        i = i+1;
    }
    return pro;
}
    
// input n and call the factorial function. 
int main(){
    int n;
    cout << "enter n:";
    cin >> n;
    cout << fac(n) << endl;

    return 0;
}
