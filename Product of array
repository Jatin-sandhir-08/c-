#include <iostream>
using namespace std;

int main(){
    int arr[] = {2,4,6,3,5,9,4,6}; //This is the array of which we have to get product.
    int size = sizeof(arr)/sizeof(arr[0]); //This helps to get the size of array.
    int i = 0, pro = 1;
    while(i <= size-1){
        pro = pro * arr[i]; //This gets the product of enteries according to the value of i from 0 to size-1.
        i = i+1;
    }
    cout << pro << endl; //Print product obtained above.

    return 0;
}
